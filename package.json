{
  "name": "ino",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "ts-node server.ts",
    "server": "nodemon --exec ts-node server.ts",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd client && npm install && npm run build"
  },
